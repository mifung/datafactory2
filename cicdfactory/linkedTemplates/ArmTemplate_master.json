{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "cicdfactory"
		},
		"Blob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Blob'"
		},
		"Sink_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": "DemoCopy2.json"
		},
		"Sink_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "datafactorymove"
		},
		"Source_properties_typeProperties_fileName": {
			"type": "string",
			"defaultValue": "DemoCopy.json"
		},
		"Source_properties_typeProperties_folderPath": {
			"type": "string",
			"defaultValue": "datafactorymove"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas Token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Blob_connectionString": {
						"value": "[parameters('Blob_connectionString')]"
					},
					"Sink_properties_typeProperties_fileName": {
						"value": "[parameters('Sink_properties_typeProperties_fileName')]"
					},
					"Sink_properties_typeProperties_folderPath": {
						"value": "[parameters('Sink_properties_typeProperties_folderPath')]"
					},
					"Source_properties_typeProperties_fileName": {
						"value": "[parameters('Source_properties_typeProperties_fileName')]"
					},
					"Source_properties_typeProperties_folderPath": {
						"value": "[parameters('Source_properties_typeProperties_folderPath')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}